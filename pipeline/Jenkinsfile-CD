@Library('shared-library') _
pipeline {
  agent any
  
   environment {
    azcon = credentials('azure-pasha')
  }
   
    stage('Download Artifact From Nexus') {
      steps {
        DownloadFromNexus()
      }
      }
    stage('Fetch Public IP') {
      steps {
        FetchPublicIP()
      }
      }
    stage('Setup Target') {
     steps {
        SetupTarget()
      }
      }
    stage('DeployToIIS') {   
      steps{
        DeployToIIS()
       }
       }
    stage('Start IIS') {
      steps {
        StartIIS()
        cleanWs()
      }
      }
     // stage('Deploy To Webapp') {
     //   steps{
     //     DeployToWebApp()
     //   }
     // }
    }
}
